<?xml version="1.0"?>
<!DOCTYPE validators PUBLIC
          "-//Apache Struts//XWork Validator 1.0.2//EN"
          "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
  <field name="name">
    <field-validator type="requiredstring">
      <message key="name.requiredstring"/>
    </field-validator>
  </field>
  
  <field name="pwd">
    <field-validator type="requiredstring">
      <message key="pwd.requiredstring"/>
    </field-validator>
  </field>
</validators>
